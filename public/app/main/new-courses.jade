.panel.panel-primary
    .panel-heading.text-center New Courses
    .panel-body
        .row(ng-repeat="course in courses | orderBy:'published':true | limitTo:10")
            .col-md-3.col-sm-3.col-xs-3 {{course.published | date:'MMM d'}}
            .col-md-9.col-sm-9.col-xs-9
            a(href="/courses/{{course._id}}") {{course.title}}